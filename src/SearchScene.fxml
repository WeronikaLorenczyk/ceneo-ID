<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SearchController">
   <children>
      <VBox layoutX="27.0" layoutY="22.0" prefHeight="358.0" prefWidth="239.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Category" />
            <ChoiceBox prefWidth="150.0" fx:id="Category" onAction="#SetCategory"/>

            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="SortBy" />
            <ChoiceBox prefWidth="150.0" fx:id="SortBy">
               <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="Lowest Price" />
               <String fx:value="Highest Price" />
               <String fx:value="Best Rating" />
            </FXCollections>
         </items>
         </ChoiceBox>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Minimal Rating" />
            <Slider fx:id="minimalRating"/>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Attribute" />
            <ChoiceBox prefWidth="150.0" fx:id="Attribute" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Location" />
            <ChoiceBox prefWidth="150.0"  fx:id="Location"/>
         </children>
      </VBox>
      <VBox layoutX="314.0" layoutY="22.0" prefHeight="358.0" prefWidth="239.0" />
   </children>
</AnchorPane>
